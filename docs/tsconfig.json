{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "jsx": "preserve",
    "paths": {
      "~/*": ["./.vitepress/vitepress/*"],
      "fish-bubble-design": ["../packages/fish-bubble-design"],
      "fish-bubble-design/shared": ["../packages/shared"]
    },
    "types": ["../typings/components"]
  },
  "include": ["**/*", ".vitepress/**/*"],
  "exclude": ["node_modules"]
}
